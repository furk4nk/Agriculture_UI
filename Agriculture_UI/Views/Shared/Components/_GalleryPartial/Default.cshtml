@using EntityLayer.Concrete
@model List<Image>
<div class="gallery" id="gallery" lang="tr">
	<div class="container">
		<h3 class="tittle">Galerimiz</h3>
		<p class="heading-btm-w3ls">Doğanın güzellikleriyle bir olun.</p>
		<div class="agile_gallery_grids w3-agile">
			<ul class="clearfix demo">
				@foreach(var item in Model)
				{
				<li>
					<div class="gallery-grid1">
						<img src="@item.ImageUrl" alt=" " class="img-responsive" />
						<div class="p-mask">
							<h4><span>@item.title</span></h4>
							<p>@item.Description</p>
						</div>
					</div>
				</li>
				}
			</ul>
		</div>
	</div>
</div>